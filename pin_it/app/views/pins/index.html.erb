<main>
  <div class="container mt-5">
    <h1 class="pl-5">Results for "<%= @search_term %>"</h1>
    <div class="container">
      <ul class="row mt-5">
        <% @pins.each do |pin|%>
          <li class="list-unstyled col-md-4 col-sm-6">
            <div class="col mb-4">
              <%= render(partial: 'pin_card', locals:{ pin: pin }) %>
            </div>
          </li>
        <% end %>
      </ul>
    </div>
  </div>
</main>

